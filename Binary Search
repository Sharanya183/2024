def sqrt(n):
    low = 0
    high = n
    while low <= high:
        mid = (low + high)/2
        if mid * mid == n:
            return mid
        elif mid * mid < n:
            low = mid - 1
        else:
            high = mid + 1
n = float(input("enter a number to find its square root: "))
print(sqrt(n))
                
                
